generator client {
  provider = "prisma-client-js"
}

// datasource db {
//   provider = "postgresql"
//   url      = env("DATABASE_URL")
// }

datasource db {
  provider = "sqlite"
  url      =  "file:./dev.db"
}

model Testimonials {
  id        Int      @id @default(autoincrement())
  comment   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("testimonials")
}